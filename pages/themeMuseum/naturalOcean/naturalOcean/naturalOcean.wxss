.main{
  width:100vw;
  height:100vh;
  display: flex;
}
/* 中国航标展馆 */
.chineseAviation{
  position: absolute;
  width: 280rpx;
  height: 182rpx;
  top:17%;
  left: 56%;
  animation:chineseAviation 1s;
  animation-timing-function: linear;
}
@keyframes chineseAviation
{
	0%{width:0rpx;height: 0rpx;top: 20%;left: 68%;}
  100% {width:280rpx;height: 182rpx;top: 17%;left: 56%;}
}
